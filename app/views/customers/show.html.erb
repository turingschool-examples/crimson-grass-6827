<%= @customer.name %>

<% @customer.items.each do |item| %>
  <%= item.name %>
  <%= item.price %>
  <%= item.supermarket.name %>
<% end %>

<%= form_with url: "/customers/#{@customer.id}", method: :patch, data: { turbo: false } do |form| %>
  <%= form.label :item_id, "Item ID" %>
  <%= form.text_field :item_id %>
  <%= form.submit "Submit" %>
<% end %>